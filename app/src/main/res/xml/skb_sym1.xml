<keyboard
  skb_template="@xml/skb_template1"
  skb_cache_flag="true"
  width="10%p"
  height="25%p"
  key_type="0"
  repeat="false"
  balloon="true">

  <row>
    <keys splitter="|" labels="!|@|#|$|%|^|&amp;|*|(|)"/>
  </row>

  <row>
    <keys splitter="|" labels="'|&quot;|=|_|:|;|?|-"/>
    <key label="|"/>
    <key label="`"/>
  </row>

  <row >
    <keys splitter="|" labels="+|-"/>
    <key label="\\"/>
    <keys splitter="|" labels="/|[|]|{|}"/>
    <key width="20%p" code="67" key_type="2" icon="@drawable/icon_delete_key" icon_popup="@drawable/icon_delete_key_pop" repeat="true"/>
  </row>

  <row >
    <key label="123" code="-4" key_type="2" width="20%p"/>
    <keys splitter="|" labels=",|.|&lt;|&gt;"/>
    <key label="abc" code="-3" key_type="2" width="20%p"/>
    <key width="20%p" code="4" label="完成" key_type="4"/>
  </row>
</keyboard>

